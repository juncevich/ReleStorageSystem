<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.relesystem.entity.Station" table="station" schema="releSystemDatabase">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="stationName">
            <column name="station_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="fkUnit">
            <column name="fk_unit" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>