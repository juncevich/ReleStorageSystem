<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.relesystem.entity.Stuff" table="stuff" schema="releSystemDatabase">
        <id name="id" column="id"/>
        <property name="stuffNumber" column="stuff_number"/>
        <property name="name" column="name"/>
        <property name="secondName" column="second_name"/>
        <property name="lastName" column="last_name"/>
        <property name="fkCategory" column="fk_category"/>
        <property name="fkUnit" column="fk_unit"/>
    </class>
</hibernate-mapping>