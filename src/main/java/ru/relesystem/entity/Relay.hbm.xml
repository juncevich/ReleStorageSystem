<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.relesystem.entity.Relay" table="relay" schema="releSystemDatabase">
        <id name="id" column="id"/>
        <property name="serialNumber" column="serial_number"/>
        <property name="manufactureDate" column="manufacture_date"/>
        <property name="lastService" column="last_service"/>
        <property name="nextService" column="next_service"/>
        <property name="responible" column="responible"/>
        <many-to-one name="stationByStationId" class="ru.relesystem.entity.Station">
            <column name="station_id"/>
        </many-to-one>
        <many-to-one name="releTypeByTypeId" class="ru.relesystem.entity.ReleType">
            <column name="type_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>